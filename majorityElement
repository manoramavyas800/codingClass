static int majority(int[] arr){
        for(int j=0;j<arr.length;j++){
            int count=1;
            for(int i=j+1;i<arr.length;i++){
                if(arr[i]==arr[j]){
                    count++;
                }
            }
            if(count>arr.length/2){
                return arr[j];
            }
        }
        return -1;
    }
